<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pyj.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:hardwareAccelerated="false"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.PieChartViewActivity"/>

        <activity android:name=".activity.HorizontalProgressBarActivity"/>

        <activity android:name=".activity.CanvasActivity"/>

        <activity android:name=".activity.CheckViewActivity"/>

        <activity android:name=".activity.PathActivity"/>

        <activity android:name=".activity.RadarViewActivity"/>

        <activity android:name=".activity.BesselActivity"/>

        <activity android:name=".activity.PathFillTypeViewActivity"/>

        <activity android:name=".activity.PathMeasureActivity"/>

        <activity android:name=".activity.SearchViewActivity"/>

    </application>

</manifest>